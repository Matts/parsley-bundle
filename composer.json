{
    "name": "j-ben87/parsley-bundle",
    "type": "symfony-bundle",
    "description": "Convert Symfony constraints into data-attributes for client-side validation with Parsley.",
    "keywords": ["symfony2", "bundle", "parsley", "validation", "client-side"],
    "license": "MIT",
    "authors": [
        {
            "name": "Benoit Jouhaud",
            "email": "bjouhaud@gmail.com",
            "homepage": "https://github.com/J-Ben87"
        }
    ],
    "require": {
        "php": ">=5.4",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "symfony/phpunit-bridge": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "JBen87\\ParsleyBundle\\": ""
        }
    },
    "minimum-stability": "stable"
